		{
			"name": "[concat(parameters('factoryName'), '/stage_Partquet_PL')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ForEachSourceParquetFile",
						"type": "ForEach",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@pipeline().parameters.table_names_array",
								"type": "Expression"
							},
							"batchCount": 1,
							"activities": [
								{
									"name": "df_parquet_to_snowflake_with_audit",
									"type": "ExecuteDataFlow",
									"dependsOn": [],
									"policy": {
										"timeout": "0.00:10:00",
										"retry": 0,
										"retryIntervalInSeconds": 60,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"dataflow": {
											"referenceName": "transform_source_parquet",
											"type": "DataFlowReference",
											"parameters": {},
											"datasetParameters": {
												"SourceParquet": {
													"container_name": "stagedata",
													"directory_name": {
														"value": "@pipeline().parameters.folder_name",
														"type": "Expression"
													},
													"file_name": {
														"value": "@concat(\n    item(),\n    '.parquet'\n)",
														"type": "Expression"
													}
												},
												"SnowflakeSink": {
													"schema_name": "STAGE",
													"table_name": {
														"value": "@toUpper(\n    item()\n)",
														"type": "Expression"
													}
												}
											}
										},
										"staging": {},
										"compute": {
											"coreCount": 8,
											"computeType": "General"
										},
										"traceLevel": "Fine"
									}
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"folder_name": {
						"type": "String",
						"defaultValue": "chinook"
					},
					"table_names_array": {
						"type": "array",
						"defaultValue": [
							"Album",
							"Artist",
							"Customer",
							"Genre",
							"Invoice",
							"InvoiceLine"
						]
					}
				},
